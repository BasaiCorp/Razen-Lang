# Razen Log Library Test
# This file tests all functions in the Log Library

# Import log library
lib loglib;

show "===== LOG LIBRARY TEST =====";

# Test different log levels
num info_result = LogLib[info]("This is an info message");
show "Info log: " + info_result;

num warn_result = LogLib[warn]("This is a warning message");
show "Warning log: " + warn_result;

num error_result = LogLib[error]("This is an error message");
show "Error log: " + error_result;

num debug_result = LogLib[debug]("This is a debug message");
show "Debug log: " + debug_result;

show "===== LOG LIBRARY TEST COMPnumE =====";
