# ====================================================================
# Math Utilities Module
# This module provides common mathematical functions and constants
# ====================================================================

# Import the standard math library
use math_lib from "math_lib";

# Define mathematical constants
let PI = 3.14159265359;
let E = 2.71828182846;

# Basic mathematical functions
fun square(x) {
    return x * x;
}

fun cube(x) {
    return x * x * x;
}

# Export the functions and constants
export PI;
export E;
export square;
export cube;
